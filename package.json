{
  "name": "azgo",
  "version": "0.0.6",
  "description": "Azure Tooling with enhanced reporting, data aggregation, and UX",
  "author": "Evan Colwell @jercle",
  "bin": {
    "azgo": "./bin/run.cjs"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "homepage": "https://github.com/jercle/azgo/blob/main/README.md",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jercle/azgo.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@azure/arm-containerregistry": "^10.0.0",
    "@azure/arm-security": "^5.0.0",
    "@azure/container-registry": "^1.0.0",
    "@azure/identity": "^3.0.0",
    "@oclif/core": "^1",
    "@oclif/plugin-commands": "^2.2.7",
    "@oclif/plugin-help": "^5",
    "axios": "^0.27.2",
    "chalk": "^5.2.0",
    "command-exists": "^1.2.9",
    "date-fns": "^2.29.3",
    "esm": "^3.2.25",
    "inquirer": "^9.1.4",
    "mongodb": "^4.14.0",
    "open": "^8.4.2",
    "yaml": "^2.2.1",
    "yeoman-environment": "^3.15.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",

    "chai": "^4",
    "eslint": "^8.24.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "^11",
    "mocha": "^10.0.0",
    "oclif": "^3.7.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "azgo",
    "dirname": "azgo",
    "commands": "./dist/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-commands"
    ],
    "topicSeparator": " ",
    "topics": {},
    "hooks": {
      "init": [
        "./dist/hooks/init/auth",
        "./dist/hooks/init/initializeApp"
      ],
      "prerun": "./dist/hooks/prerun/prerun"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/jercle/azgo/issues",
  "keywords": [
    "azure",
    "vulnerability",
    "security",
    "azure-security",
    "azure-vulnerability",
    "azure-security-center"
  ],
  "types": "dist/index.d.ts"
}
